From c1c6b5aff25bfde1533886952a633980470e100f Mon Sep 17 00:00:00 2001
From: jenslody <jenslody@2a5c6006-c6dd-42ca-98ab-0921f2732cef>
Date: Tue, 24 Dec 2013 12:19:54 +0000
Subject: [PATCH 1/5] * Prepare release (remove release-candidate tags)

---
 codeblocks.spec.in   | 4 +++-
 debian/changelog     | 6 ++++++
 src/src/appglobals.h | 2 +-
 update_revision.sh   | 4 ++--
 4 files changed, 12 insertions(+), 4 deletions(-)
 mode change 100755 => 100644 update_revision.sh

diff --git a/codeblocks.spec.in b/codeblocks.spec.in
index 2998491..2092c26 100644
--- a/codeblocks.spec.in
+++ b/codeblocks.spec.in
@@ -4,7 +4,7 @@
 %define _redhat             %(if [ -f /etc/redhat-release ]; then echo 1; else echo 0; fi)
 %define _mandrake           %(if [ -f /etc/mandrake-release ]; then echo 1; else echo 0; fi)
 %define _version            13.12
-%define _rel                rc2
+%define _rel                1
 %define _release            %{_rel}
 %define jobs                %(if [ -e /usr/bin/getconf ]; then echo $(( `/usr/bin/getconf _NPROCESSORS_ONLN` + 1 )); else echo 1; fi)
 
@@ -429,6 +429,8 @@ This package contains the development libraries and headers needed to build wxSm
 %{_includedir}/wxsmith/*
 
 %changelog
+* Tue Dec 24 2013 Jens Lody <jens@codeblocks.org>
+- Changes for release 13.12.
 * Sun Dec 08 2013 Jens Lody <jens@codeblocks.org>
 - Second release candidate of release 13.12.
 * Sun Nov 17 2013 Jens Lody <jens@codeblocks.org>
diff --git a/debian/changelog b/debian/changelog
index a22348c..daa4b7a 100644
--- a/debian/changelog
+++ b/debian/changelog
@@ -1,3 +1,9 @@
+codeblocks (13.12-1) unstable; urgency=low
+
+  * Changes for release 13.12.
+
+ -- Jens Lody <jens@codeblocks.org>  Tue, 24 Dec 2013 13:12:58 +0100
+
 codeblocks (13.12-rc2) unstable; urgency=low
 
   * Second release candidate of release 13.12.
diff --git a/src/src/appglobals.h b/src/src/appglobals.h
index 68e7655..6924a1a 100644
--- a/src/src/appglobals.h
+++ b/src/src/appglobals.h
@@ -9,7 +9,7 @@
 #include <wx/string.h>
 #include <wx/intl.h>
 
-#define RELEASE "13.12-RC2"
+#define RELEASE "13.12"
 
 #ifndef RELEASE
     #define RELEASE "svn"
diff --git a/update_revision.sh b/update_revision.sh
old mode 100755
new mode 100644
index e2b0f9d..2894113
--- a/update_revision.sh
+++ b/update_revision.sh
@@ -56,12 +56,12 @@ if [ "x$REV" != "x$OLD_REV" -o ! -r $REV_FILE ]; then
 		if [ -x "$DCH" ]; then
 			AKT_REV=`sed -e 's/.*svn\([0-9]*\).*/\1/' -e 'q' < debian/changelog`
 			if [ $REV -gt $AKT_REV ]; then
-				dch -v 13.12-rc2 "Second release candidate of release 13.12."
+				dch -v 13.12-1 "Changes for release 13.12."
 			fi
 		fi
 	else
 		mv debian/changelog debian/changelog.tmp
-		sed "1 s/(13.12svn[^-)]*/(13.12-rc2/" < debian/changelog.tmp > debian/changelog
+		sed "1 s/(13.12svn[^-)]*/(13.12-1/" < debian/changelog.tmp > debian/changelog
 		rm debian/changelog.tmp
 	fi
 fi
-- 
2.1.4


